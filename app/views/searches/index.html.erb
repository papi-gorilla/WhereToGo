<div class = "container px-5 px-sm-0">
  <div class = "row">
    <h3 class = "mx-auto">検索結果一覧画面</h3>
  </div>

  <div class = "row">
    <div class = "col">
      <% if @range == "Post" %>
        <% @posts.each do |post| %>
          <div class = "post_<%= post.id %>">
            <%= render "posts/post_index", post: post %>
          </div>
        <% end %>
      <% else %>
        <% @users.each do |user| %>
          <%= user.user_name %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>